{
    "name": "privEd",
    "description" : "A privacy literacy Chrome Extension",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage", "declarativeContent", "activeTab", "*://*/*", "tabs", "cookies", "webNavigation", "identity", "identity.email"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/hello_extensions.png"
    },
    "background":{
      "scripts": ["js/jquery.js", "js/background.js"],
      "persistent": false
    },

    "content_scripts": [
      {
        "js": ["js/jquery.js", "js/cookies.js", "js/content.js"],
        "matches": ["*://*/*"]
      }
    ],

    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    }
  }